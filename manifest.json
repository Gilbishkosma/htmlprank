{
	"manifest_version":2,
	"name":"HTML Prank",
	"version":"1.0",
	"description": "Change The content of any website using HTMLPrank",
	"author":"Gilbish Kosma",



	"content_scripts":[
		{
			"matches":[
				"<all_urls>"
			],
			"js":["content.js"]
		}
	],
	"permissions":["activeTab"],
	"browser_action":{
		"default_icon":"cat.png",
		"default_popup":"popup.html",
		"default_title":"A POPUP will come here"
	}
}